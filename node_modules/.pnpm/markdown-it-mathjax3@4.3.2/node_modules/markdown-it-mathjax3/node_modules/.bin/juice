#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/juice@8.1.0/node_modules/juice/bin/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/juice@8.1.0/node_modules/juice/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/juice@8.1.0/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/juice@8.1.0/node_modules/juice/bin/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/juice@8.1.0/node_modules/juice/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/juice@8.1.0/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../juice@8.1.0/node_modules/juice/bin/juice" "$@"
else
  exec node  "$basedir/../../../../../juice@8.1.0/node_modules/juice/bin/juice" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/speech-rule-engine@4.1.2/node_modules/speech-rule-engine/bin/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/speech-rule-engine@4.1.2/node_modules/speech-rule-engine/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/speech-rule-engine@4.1.2/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/speech-rule-engine@4.1.2/node_modules/speech-rule-engine/bin/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/speech-rule-engine@4.1.2/node_modules/speech-rule-engine/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/speech-rule-engine@4.1.2/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../speech-rule-engine@4.1.2/node_modules/speech-rule-engine/bin/sre" "$@"
else
  exec node  "$basedir/../../../../../speech-rule-engine@4.1.2/node_modules/speech-rule-engine/bin/sre" "$@"
fi

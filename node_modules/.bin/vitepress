#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/vitepress@1.0.0-rc.40_@algolia+client-search@5.35.0_markdown-it-mathjax3@4.3.2_postcss@_12a8a802981995f6ba04384f772eddc3/node_modules/vitepress/bin/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/vitepress@1.0.0-rc.40_@algolia+client-search@5.35.0_markdown-it-mathjax3@4.3.2_postcss@_12a8a802981995f6ba04384f772eddc3/node_modules/vitepress/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/vitepress@1.0.0-rc.40_@algolia+client-search@5.35.0_markdown-it-mathjax3@4.3.2_postcss@_12a8a802981995f6ba04384f772eddc3/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/vitepress@1.0.0-rc.40_@algolia+client-search@5.35.0_markdown-it-mathjax3@4.3.2_postcss@_12a8a802981995f6ba04384f772eddc3/node_modules/vitepress/bin/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/vitepress@1.0.0-rc.40_@algolia+client-search@5.35.0_markdown-it-mathjax3@4.3.2_postcss@_12a8a802981995f6ba04384f772eddc3/node_modules/vitepress/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/vitepress@1.0.0-rc.40_@algolia+client-search@5.35.0_markdown-it-mathjax3@4.3.2_postcss@_12a8a802981995f6ba04384f772eddc3/node_modules:/Users/yulinzhu/Desktop/vitePress-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
